{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "noImplicitAny": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "lib": ["ES2020", "DOM", "DOM.Iterable"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }